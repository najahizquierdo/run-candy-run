<%= form_for @location do |f| %>
   <%= f.label :title %>
   <%= f.text_field :title %> <p>
   <%= f.label :description %>
   <%= f.text_field :description %> <p>
   <%= f.label :address %>
   <%= f.text_field :address %>
   <%= f.label :date %>
   <%= f.date_field :date %>
   <%= f.label :distance %>
   <%= f.text_field :distance %>
   <%= f.label :time %>
   <%= f.text_field :time %>
   <%= f.submit 'Add' %>
<%end %>
